{
  "name": "ev-community-backend",
  "version": "1.0.0",
  "description": "Backend API for EV Community Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "test": "node tests/runTests.js",
    "test:jest": "jest",
    "test:watch": "node tests/runTests.js --watch",
    "test:coverage": "node tests/runTests.js --coverage",
    "test:auth": "node tests/runTests.js --suite=auth",
    "test:users": "node tests/runTests.js --suite=users",
    "test:vehicles": "node tests/runTests.js --suite=vehicles",
    "test:charging": "node tests/runTests.js --suite=charging",
    "test:forum": "node tests/runTests.js --suite=forum",
    "test:marketplace": "node tests/runTests.js --suite=marketplace",
    "test:notifications": "node tests/runTests.js --suite=notifications",
    "test:admin": "node tests/runTests.js --suite=admin",
    "test:parallel": "node tests/runTests.js --parallel",
    "test:bail": "node tests/runTests.js --bail",
    "test:silent": "node tests/runTests.js --silent",
    "test:debug": "DEBUG=* node tests/runTests.js --verbose",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "migrate": "node migrations/migrate.js",
    "seed": "node seeds/seed.js"
  },
  "keywords": [
    "ev",
    "community",
    "api",
    "express",
    "supabase"
  ],
  "author": "EV Community Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "path": "^0.12.7",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
